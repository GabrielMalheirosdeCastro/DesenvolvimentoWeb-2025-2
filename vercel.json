{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/galeria-lego-naves.html",
      "destination": "/galeria-lego-naves.html"
    },
    {
      "source": "/javascript-fundamentals.html",
      "destination": "/javascript-fundamentals.html"
    },
    {
      "source": "/boxmodel.html",
      "destination": "/boxmodel.html"
    },
    {
      "source": "/flexbox.html",
      "destination": "/flexbox.html"
    },
    {
      "source": "/responsivo.html",
      "destination": "/responsivo.html"
    },
    {
      "source": "/posicionamento.html",
      "destination": "/posicionamento.html"
    },
    {
      "source": "/tipografia.html",
      "destination": "/tipografia.html"
    },
    {
      "source": "/lab-fundamentos-css.html",
      "destination": "/lab-fundamentos-css.html"
    },
    {
      "source": "/matematica-operadores.html",
      "destination": "/matematica-operadores.html"
    },
    {
      "source": "/desafio-cores/index.html",
      "destination": "/desafio-cores.html"
    },
    {
      "source": "/desafio-cores",
      "destination": "/desafio-cores.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}